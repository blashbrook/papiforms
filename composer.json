{
    "name": "blashbrook/papiforms",
    "description": "Custom Forms for the Polaris ILS",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Lashbrook",
            "email": "blashbrook@gmail.com",
            "homepage": "https://github.com/blashbrook"
        }
    ],
    "homepage": "https://github.com/blashbrook/papiforms",
    "keywords": ["Laravel", "PAPIForms"],
    "require": {
        "php": "^8.2",
        "livewire/livewire": "^v3.4.12",
        "league/flysystem-aws-s3-v3": "^3.10",
        "blashbrook/papiclient": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~10",
        "orchestra/testbench": "^8.36",
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "files": [
            "src/PAPIForms.php"
        ],
        "psr-4": {
            "Blashbrook\\PAPIForms\\": "src/",
            "Blashbrook\\PAPIForms\\App\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blashbrook\\PAPIForms\\Tests\\": "src/Tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blashbrook\\PAPIForms\\App\\Providers\\PAPIFormsServiceProvider"
            ],
            "aliases": {
                "PAPIForms": "Blashbrook\\PAPIForms\\Facades\\PAPIForms",
                "DeliveryOptionController": "Blashbrook\\PAPIForms\\Facades\\DeliveryOptionController",
                "MobilePhoneCarrierController": "Blashbrook\\PAPIForms\\Facades\\MobilePhoneCarrierController",
                "PatronCodeController": "Blashbrook\\PAPIForms\\Facades\\PatronCodeController",
                "PostalCodeController": "Blashbrook\\PAPIForms\\Facades\\PostalCodeController",
                "UdfOptionController": "Blashbrook\\PAPIForms\\Facades\\UdfOptionController"
            }
        }
    }
}
